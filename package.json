{
  "name": "PicPicker",
  "version": "1.0.0",
  "description": "Easily copy pictures from one folder to another.",
  "main": "./main.js",
  "scripts": {
    "start": "./node_modules/.bin/sass styles.scss styles.css && ./node_modules/.bin/electron .",
    "compile": "sass styles.scss styles.css",
    "dist": "electron-builder -wl"
  },
  "repository": "https://github.com/Nootaku/Picpicker",
  "keywords": [
    "Electron",
    "Clickado",
    "Picpicker",
    "demo"
  ],
  "author": "Maxime WATTEZ <max.wattez@protonmail.com>",
  "license": "GNU GENERAL PUBLIC LICENSE",
  "devDependencies": {
    "electron": "^9.4.4",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "sass": "^1.42.0"
  },
  "build": {
    "appId": "com.docenhance.picpicker",
    "copyright": "Copyright Â© 2020 MAXIME WATTEZ",
    "productName": "PicPicker",
    "electronVersion": "9.0.5",
    "linux": {
      "target": [
        "appImage",
        "deb"
      ],
      "category": "Utility"
    },
    "win": {
      "target": "nsis"
    }
  }
}
